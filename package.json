{
   "name": "test-react-library",
   "version": "1.0.0",
   "description": "Simple example of react library",
   "author": "jmorozov",
   "license": "MIT",
   "repository": "jmorozov/test-react-library",
   "main": "dist/index.js",
   "module": "dist/index.es.js",
   "jsnext:main": "dist/index.es.js",
   "engines": {
      "node": ">=8",
      "npm": ">=5"
   },
   "scripts": {
      "test": "react-scripts test",
      "test:watch": "react-scripts test",
      "build": "rollup -c",
      "start": "rollup -c -w && npm run prettier-watch",
      "prepare": "npm run build",
      "predeploy": "cd example && npm install && npm run build",
      "deploy": "gh-pages -d example/build",
      "prettier-watch": "onchange 'src/**/*' -- prettier --write {{changed}}",
      "stlg": "styleguidist server",
      "stlg:build": "styleguidist build"
   },
   "dependencies": {},
   "peerDependencies": {
      "prop-types": "^15.7.2",
      "react": "^16.0.0",
      "react-dom": "^16.0.0"
   },
   "devDependencies": {
      "@svgr/rollup": "^4.3.1",
      "@types/jest": "^24.0.15",
      "@types/react": "^16.8.23",
      "@types/react-dom": "^16.8.4",
      "gh-pages": "^2.0.1",
      "husky": "^3.0.0",
      "lint-staged": "^9.1.0",
      "onchange": "6.0.0",
      "prettier": "1.18.2",
      "react": "^16.8.6",
      "react-docgen-typescript": "^1.12.5",
      "react-dom": "^16.8.6",
      "react-scripts": "^3.0.1",
      "react-styleguidist": "^9.1.11",
      "rollup": "^1.16.6",
      "rollup-plugin-babel": "^4.3.3",
      "rollup-plugin-commonjs": "^10.0.1",
      "rollup-plugin-node-resolve": "^5.2.0",
      "rollup-plugin-peer-deps-external": "^2.2.0",
      "rollup-plugin-postcss": "^2.0.3",
      "rollup-plugin-typescript2": "^0.22.0",
      "rollup-plugin-url": "^2.2.2",
      "typescript": "^3.5.2",
      "webpack-blocks": "2.0.1"
   },
   "files": [
      "dist"
   ],
   "husky": {
      "hooks": {
         "pre-commit": "lint-staged"
      }
   },
   "lint-staged": {
      "*.{js,ts,tsx,css,json,md}": [
         "prettier --write",
         "git add"
      ]
   }
}
